const data1 = new Promise((resolve, reject) => {
    setTimeout(() => {
        resolve("Data loaded successfully");
    }, 2000);
});

data1
    .then((message) => {
        console.log(message);
    })
    .catch((error) => {
        console.log(error);
    });

async function handleData() {
    try {
        const message = await data1;
        console.log(message);
    } catch (error) {
        console.log("Error:", error);
    }
}

handleData();
